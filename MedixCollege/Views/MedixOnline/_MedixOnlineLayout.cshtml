@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    ViewBag.Title = "Medix Online";
}

<div class="leftMenuWrapper col-lg-3 custom-collapse">
    <button class="btn-brand collapse-toggle visible-xs" type="button" data-toggle="collapse" data-parent="custom-collapse" data-target="#side-menu-medix">
        <span class="">CAMPUSES</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
    </button>
    <ul class="leftMenu list-unstyled collapse" id="side-menu-medix">
        <li class="item contentH1-white">@Html.ActionLink("About Medix Online", "Index", "MedixOnline")</li>
        <li class="item side-menu-h2">@Html.ActionLink("Medix Online Programs", "Index", "MedixOnline")</li>
        <li class="item side-menu-h2">@Html.ActionLink("Schedules and Fees", "SchedulesAndFees", "MedixOnline")</li>
        <li class="item side-menu-h2">@Html.ActionLink("E-Learning", "ELearning", "MedixOnline")</li>
        <li class="item side-menu-h2">@Html.ActionLink("Advantages", "Advantages", "MedixOnline")</li>
        <li class="item side-menu-h2">@Html.ActionLink("Contact Medix Online", "Index", "MedixOnline")</li>
    </ul>
    <br />
    <ul class="leftMenu list-unstyled collapse" id="side-menu-contact">
        <li class="item btn btn-brand btn-block spacer-top" id="askQuestion">Ask a Question</li>
        <li class="item btn btn-brand btn-block" id="requestCatalogue">Request Information</li>
        <li class="item btn btn-brand btn-block" id="applyOnline">Apply Online</li>
    </ul>
</div>
<div class="col-lg-9">
    <div class="contentHeader">
        Medix Online
    </div>
    <div class="contentBody">
        @RenderBody()
    </div>
</div>
<div class="clearfix"></div>
